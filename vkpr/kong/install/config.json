{
  "inputs": [
    {
      "label": "Type the Kong domain:",
      "name": "domain",
      "type": "text",
      "default": "localhost"
    },
    {
      "label": "Secure?",
      "name": "secure",
      "type": "bool",
      "default": "false",
      "items": [
        "true",
        "false"
      ]
    },
    {
      "label": "Kong Mode: (Hybrid only available in Kong Enterprise)",
      "name": "kong_mode",
      "type": "text",
      "default": "dbless",
      "items": [
        "standard",
        "hybrid",
        "dbless"
      ]
    },
    {
      "label": "Do you will use enterprise?",
      "name": "enterprise",
      "type": "bool",
      "required": true,
      "default": "true",
      "items": [
        "true",
        "false"
      ]
    },
    {
      "cache": {
        "active": true,
        "qty": 2,
        "newLabel": "Type another license path: "
      },
      "condition": {
        "variable": "enterprise",
        "operator": "==",
        "value": "true"
      },
      "label": "Type the full path to the license:",
      "name": "license",
      "default": " ",
      "type": "text"
    },
    {
      "condition": {
        "variable": "enterprise",
        "operator": "==",
        "value": "true"
      },
      "label": "Type the RBAC Admin Password:",
      "name": "rbac_password",
      "default": "vkpr123",
      "type": "text"
    },
    {
      "default": "false",
      "items": [
        "true",
        "false"
      ],
      "label": "HA? ",
      "name": "HA",
      "type": "bool"
    }
  ],
  "template": "shell-bat",
  "templateRelease:": "2.16.2"
}
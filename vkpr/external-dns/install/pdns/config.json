{
  "dockerImageBuilder": "ritclizup/rit-shell-bat-builder",
  "inputs": [
    {
      "label": "What is the powerdns server apiurl endpoint?",
      "name": "apiurl",
      "required": true,
      "type": "text",
      "cache": {
        "active": true,
        "qty": 5,
        "newLabel": "Type another url:"
      }
    },
    {
      "name": "apikey",
      "type": "CREDENTIAL_POWERDNS_APIKEY"
    }
  ],
  "template": "shell-bat",
  "templateRelease:": "2.16.2"
}
